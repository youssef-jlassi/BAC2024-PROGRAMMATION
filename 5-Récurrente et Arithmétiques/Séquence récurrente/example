def fibonacci(n):
    if n <= 0:
        return "Veuillez entrer un entier positif."
    elif n == 1:
        return 0
    elif n == 2:
        return 1
    else:
        return fibonacci(n-1) + fibonacci(n-2)

# Tester la sÃ©quence de Fibonacci pour les 10 premiers termes
for i in range(1, 11):
    print(f"Fibonacci({i}) = {fibonacci(i)}")
